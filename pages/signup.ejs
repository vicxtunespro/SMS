<%- include('partials/header') %>
  <div class="container">
    <form action="/user" method="POST" id="admission-form">
        <h1 style="color: white;">Sign Up</h1>
        <div class="form-group">
            <input class="field" type="text" name="firstName" placeholder="FirstName" autocomplete="off">
            <input class="field" type="text" name="lastName" placeholder="lastName" autocomplete="off">
            <input class="field" type="text" name="userName" placeholder="userName" autocomplete="off">
        </div>
        <div class="form-group">
            <input class="field" type="password" name="password" placeholder="password">
            <input class="field" type="password" name="confirm-password" placeholder="password">
        </div>
        <button type="subit">Register</button>
    </form>
  </div>

<%- include('partials/footer')%>